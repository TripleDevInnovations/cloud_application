DirectoryFileService:
    - move to path
    - show elements
    - upload File
    - create File
    - rename File
    - move element

Controller:
    - /login
    - /{user}
    - /{user}/{path}
    - /{user}/{path}/download
    - /{user}/{path}/upload
    - /{user}/{path}/move
    - /{user}/{path}/rename

